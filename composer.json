{
    "name": "hollow3464/alegra-api-psr",
    "description": "Librería PSR para la integración de la API de Alegra",
    "type": "library",
    "version": "1.2.1",
    "scripts": {
        "format": "vendor/bin/pint",
        "lint": "vendor/bin/phpstan"
    },
    "autoload": {
        "psr-4": {
            "Hollow3464\\Alegra\\": "src/"
        }
    },
    "authors": [
        {
            "name": "David Chamorro",
            "email": "daved3464@gmail.com"
        }
    ],
    "require": {
        "psr/log": "^3.0",
        "psr/http-client": "^1.0.2",
        "psr/http-factory": "^1.0.2",
        "psr/http-message": "^1.1|^2.0",
        "psr/event-dispatcher": "^1.0",
        "fig/http-message-util": "^1.1.5",
        "cuyz/valinor": "^1.4"
    },
    "require-dev": {
        "laravel/pint": "^1.10",
        "phpstan/phpstan": "^2.1"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
