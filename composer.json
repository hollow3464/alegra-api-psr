{
    "name": "save-colombia/alegra_api_psr",
    "description": "Librería PSR para la integración de la API de Alegra",
    "type": "library",
    "autoload": {
        "psr-4": {
            "SaveColombia\\AlegraApiPsr\\": "src/"
        }
    },
    "authors": [
        {
            "name": "David Chamorro",
            "email": "soporte@savecol.com"
        }
    ],
    "require": {
        "psr/log": "^3.0",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^2.0",
        "psr/event-dispatcher": "^1.0",
        "fig/http-message-util": "^1.1",
        "cuyz/valinor": "^1.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.17",
        "sqlftw/sqlftw": "^0.1.11",
        "phpstan/extension-installer": "^1.3",
        "symplify/easy-coding-standard": "*"
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
